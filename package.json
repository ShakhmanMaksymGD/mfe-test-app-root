{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "run:all": "concurrently \"npm run dev\" \"npm run dev:sspa --prefix ../monthly\" \"npm run dev --prefix ../shared\" \"npm run build --prefix ../yearly\" \"npm run preview --prefix ../yearly\"",
